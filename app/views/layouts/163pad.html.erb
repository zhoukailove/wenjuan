<script>
  var image_url = $('#image_url').data('src');
  console.info(image_url);
  $('#motionbg').css("backgroundImage","url("+image_url+")");
  function fIpadIndex(sBgUrl) {
    var oIndex = {
      init: fInit // 大背景初始化
      , loginInit: fLoginInit // 登录逻辑初始化
//      , bgUrl: 'sBgUrl' // 背景图URL
      , bgUrl: image_url // 背景图URL
      , ratio: 1.3 // 放大比率
      , id: fGetElementById
      , isDeviceMotion: false
      , position: { // 位置
        x: 0
        , y: 0
      }
      , acceleration: { // 加速度
        x: 0
        , y: 0
      }
      , bUseMotion: false
      , setOriEvent: fSetOrientationEvent // Orientation 事件
      , bUseOrientation: false
      , setMotionEvent: fSetMotionEvent // motion 事件
      , setBackground: fSetBackgroundStyle // 初始化背景图
      , setPosition: fSetPosition // 设置背景图位置
      , setPositionByOrien: fSetPositionByOrientation
      , getOrientation: fGetOrientation // 获取方向
      , orientation: '' // 初始方向
      , isUp: false
      , isInit: false
      , preload: fPreloadImg // 预加载图片
    };

    window.$ = oIndex.id;

    function fLoginInit() {
      var o = this;
      var oConf = {
        action: 'https://urs.mail.163.com/entry/proxy?type=1&product=mail163&url=http%3A%2F%2Fentry.mail.163.com%2Fcoremail%2Ffcg%2Fntesdoor2%3Flightweight%3D1%26verifycookie%3D1%26language%3D0%26style%3D11%26destip%3D192.168.193.48'
        ,
        cookieDomain: 'mail.163.com'
        ,
        mailDomain: '163.com'
        ,
        autoLoginUrl: 'http://entry.mail.163.com/coremail/fcg/ntesdoor2?lightweight=1&verifycookie=1&language=0&style=11&destip=192.168.193.48'
      };

      // 判断df
      _fSetEntryDf('pad');

      // 自动登录检测
      _fAutoLogin();

      var oForm = $('loginForm');
      oForm.action = oConf.action;
      oForm.onsubmit = function () {
        return _fLogin();
      }

      var oSubmit = $('iptSubmit');
      oSubmit.addEventListener('touchstart', function () {
        oSubmit.className = 'ipt-submit ipt-submit-active';
      }, false);
      oSubmit.addEventListener('touchend', function () {
        oSubmit.className = 'ipt-submit';
      }, false);

      var oUsernameWidth = $('usernameWidth');
      var oUserDomain = $('userDomain');
      var oUserNameIpt = $('usernameIpt');
      var oUserName = $('username');
      oUserDomain.innerHTML = '@' + oConf.mailDomain;
      oUserNameIpt.addEventListener('focus', function () {
        $('iptUsername').className = 'ipt ipt-focus';
        _fUserNameWidth();
      }, false);
      oUserNameIpt.addEventListener('input', function () {
        _fUserNameWidth();
      }, false);
      oUserNameIpt.addEventListener('blur', function () {
        $('iptUsername').className = 'ipt';
        if (oUserNameIpt.value == '') {
          oUserDomain.style.display = 'none';
        }
      }, false);

      var oPassWord = $('password');
      oPassWord.addEventListener('focus', function () {
        $('iptPassword').className = 'ipt ipt-pw ipt-focus';
      }, false);
      oPassWord.addEventListener('blur', function () {
        $('iptPassword').className = 'ipt ipt-pw';
      }, false);

      var oLoginCkb = $('loginSaveLogin');
      var oRmbCkb = $('chkRemember');
      $('iptLabArea').addEventListener('touchstart', function () {
        _fCbkCheck();
      }, false);

      function _fCbkCheck() {
        var bSwitch = oRmbCkb.checked;
        oRmbCkb.checked = !oRmbCkb.checked;
        if (oRmbCkb.checked) {
          oLoginCkb.className = 'ipt-lab ipt-lab-checked';
        } else {
          oLoginCkb.className = 'ipt-lab';
        }
      }

      function _fUserNameWidth() {
        if (oUserNameIpt.value == '') {
          oUserDomain.style.display = 'none';
        } else {
          var nWidth = 0;
          var sLength = oUserNameIpt.value.length;
          sLength = sLength * 3;
          oUserDomain.style.display = 'block';
          oUsernameWidth.innerHTML = oUserNameIpt.value;
          nWidth = oUsernameWidth.offsetWidth;
          oUserDomain.style.left = nWidth + sLength + 6 + 'px';
        }
      }

      function _fTrim(str) {
        return str.replace(/(^\s*)|(\s*$)/g, "");
      }

      //
      function _fLogin() {
        var sUser = _fTrim($('usernameIpt').value);
        var sPass = _fTrim($('password').value);
        var oCbx = $('chkRemember');
        var oSaveLogin = $('saveLogin');
        if (sUser == '') {
          alert('请填写用户名！');
          $('usernameIpt').focus();
          return false;
        }

        var sUsername = $('usernameIpt').value;
        if (sUsername.indexOf('@') > -1) {
          sUsername = sUsername.split('@')[0];
        }

        oUserName.value = sUsername + '@' + oConf.mailDomain;

        if (sPass == '') {
          alert('请填写密码！');
          $('password').focus();
          return false;
        }
        if (oCbx.checked) {
          oSaveLogin.value = 1;
        }
        else {
          oSaveLogin.value = 0;
        }
        return true;
      }

      //两周自动登录
      function _fAutoLogin() {
        try {
          var sSInfo = _fGetCookie("S_INFO");
          var sPInfo = _fGetCookie("P_INFO");
          var sEmail = "", sTime = "", sState = "", nTimeDiff = 0;
          if (sPInfo) {
            var aInfo = sPInfo.split("|");
            sEmail = aInfo[0];
            sTime = aInfo[1];
            sState = aInfo[2];
            nTimeDiff = (new Date()).getTime() - (sTime + "000");
          }
          if (!location.hash && sEmail.indexOf("@" + oConf.mailDomain) > -1 && sState == "1" && nTimeDiff < 14 * 24 * 60 * 60 * 1000) {
            window.location.href = oConf.autoLoginUrl;
          }
        } catch (exp) {
        }
      }

      //获取Cookie
      function _fGetCookie(sName) {
        var sSearch = sName + "=";
        if (document.cookie.length > 0) {
          offset = document.cookie.indexOf(sSearch)
          if (offset != -1) {
            offset += sSearch.length;
            end = document.cookie.indexOf(";", offset)
            if (end == -1) end = document.cookie.length;
            return unescape(document.cookie.substring(offset, end))
          }
          else return ""
        }
      }

      // _fSetEntryDf
      function _fSetEntryDf(sFrom) {
        var sOpreation = 'ios';
        if (navigator.userAgent.toLowerCase().indexOf('android') > -1) {
          sOpreation = 'android';
        }
        var sDf = '&df=' + sFrom + '_' + sOpreation;
        var sDfEncode = encodeURIComponent(sDf);
        oConf.action = oConf.action + sDfEncode;
        oConf.autoLoginUrl = oConf.autoLoginUrl + sDf;
      }
    }

    function fGetOrientation() {
      var o = this;
      var sOrientation = '';
      switch (window.orientation) {
        case -90:
          sOrientation = 'landscape';
          o.isUp = true;
          break
        case 90:
          sOrientation = 'landscape';
          o.isUp = false;
          break;
        case -180:
          sOrientation = 'portrait';
          o.isUp = false;
          break;
        case 180:
          sOrientation = 'portrait';
          o.isUp = true;
          break;
        default:
          sOrientation = 'portrait';
          break;
      }
      if (sOrientation != o.orientation) {
        o.orientation = sOrientation;
        if (o.isInit) {
          o.init();
        }
      }
    }

    function fSetPositionByOrientation(x, y, bInit) {
      var o = this;
      var nRealX;
      var nRealY;
      if (o.orientation == 'landscape') {
        nRealX = y;
        nRealY = x;
      } else {
        nRealX = x;
        nRealY = y;
      }
      o.mbg.style.webkitTransform = 'translate3d(' + nRealX + 'px, ' + nRealY + 'px, 0)';
      //o.mbg.style['-webkit-transition']  = 'translate3d(' + Math.round(nRealX) + 'px, ' +  Math.round(nRealY) + 'px, 0) 1s ease-in';
    }

    function fSetPosition() {
      var o = this;
      var nAccX = o.acceleration.x;
      var nAccY = o.acceleration.y;
      var nTmpX, nTmpY;
      if (nAccX) {
        if (!o.isUp) {
          nAccX = 0 - nAccX;
        }
        if (o.orientation == 'landscape') {
          nAccX = 10 - nAccX;
        } else {
          nAccX = 5 - nAccX;
        }
        // new
        nAccX = nAccX * 0.1;
        if (nAccX < 0) {
          nAccX = 0;
        }
        if (nAccX > 1) {
          nAccX = 1;
        }
        nTmpX = o.range.x * nAccX;
        if (nTmpX <= 0 && nTmpX >= o.range.x) {
          o.position.x = nTmpX;
        }
        // new
        /**
         nTmpX = o.position.x + nAccX;
         if(nTmpX <= 0 && nTmpX >= o.range.x){
						o.position.x = nTmpX;
					}
         */
      }
      if (nAccY) {
        if (!o.isUp) {
          if (o.orientation == 'landscape') {
            nAccY = 0 - nAccY;
          }
        }
        if (o.orientation == 'landscape') {
          nAccY = 5 - nAccY;
        } else {
          nAccY = 10 - nAccY;
        }
        // new
        nAccY = nAccY * 0.1;
        if (nAccY < 0) {
          nAccY = 0;
        }
        if (nAccY > 1) {
          nAccY = 1;
        }
        nTmpY = o.range.y * nAccY;
        if (nTmpY <= 0 && nTmpY >= o.range.y) {
          o.position.y = nTmpY;
        }
        // new
        /**
         nTmpY = o.position.y + nAccY;
         if(nTmpY <= 0 && nTmpY >= o.range.y){
						o.position.y = nTmpY;
					}
         */
      }
      // 赋值
      o.setPositionByOrien(o.position.x, o.position.y);
    }

    function fSetMotionEvent() {
      var o = this;
      o.bUseMotion = true;
      window.ondevicemotion = function (event) {
        if (o.bUseMotion) {
          o.acceleration.x = event.accelerationIncludingGravity.x;
          o.acceleration.y = event.accelerationIncludingGravity.y;
          o.acceleration.z = event.accelerationIncludingGravity.z;
          o.setPosition();
        }
      };
    }

    function fSetOrientationEvent() {
      var o = this;
      alert(window.ondeviceorientation);
      if (window.ondeviceorientation == undefined) {
        return;
      } else {
        o.bUseOrientation = true;
        o.bUseMotion = false;
        window.ondeviceorientation = function (event) {
          if (o.bUseOrientation) {
            var alpha = Math.round(event.alpha);
            var beta = Math.round(event.beta);
            var gamma = Math.round(event.gamma);
            $('usernameIpt').value = alpha + ' ' + beta + ' ' + gamma;
          }
        }
      }
    }

    function fPreloadImg() {
      var o = this;
      if (!o.bgUrl) {
        o.bgUrl = 'http://mimg.127.net/index/pad/img/163bg.jpg';
      }
      _fImgLoader(o.bgUrl, true);

      function _fImgLoader(imgSrc, fSuccCallBack, fErrorCallBack, nTimeout) {
        var oImg = document.createElement('IMG');
        if (fSuccCallBack) {
          oImg.onload = function () {
            // 保存图片信息
            o.bgImg = {};
            o.bgImg.src = oImg.src;
            o.bgImg.width = oImg.width;
            o.bgImg.height = oImg.height;

            // 初始化
            o.init();
          };
        }
        if (fErrorCallBack) {
          oImg.onerror = function () {
            fErrorCallBack();
          };
        }
        var nTime = 0;
        if (nTimeout) {
          nTime = nTimeout;
        }
        setTimeout(function () {
          oImg.src = imgSrc;
        }, nTime);
      }
    }

    function fSetBackgroundStyle() {
      var o = this;
      // 背景图原始宽高
      var nWidth = o.bgImg.width;
      var nHeight = o.bgImg.height;
      // 背景图临时宽高
      var nTmpWidth = 0;
      var nTmpHeight = 0;
      // 浏览器宽高
      var nDomWidth = document.documentElement.clientWidth;
      var nDomHeight = document.documentElement.clientHeight;
      // 比率
      var nRatio = 0;
      // 处理图片
      if (nWidth >= nHeight) {
        // 如果图片 宽 > 高 尝试拉伸
        nRatio = nDomHeight / nHeight;
        nTmpHeight = nDomHeight;
        nTmpWidth = nWidth * nRatio;
      } else {
        nRatio = nDomWidth / nWidth;
        nTmpWidth = nDomWidth;
        nTmpHeight = nHeight * nRatio;
      }
      // 再次调整
      if (nTmpWidth < nDomWidth) {
        nRatio = nDomWidth / nTmpWidth;
        nTmpWidth = nDomWidth;
        nTmpHeight = nDomHeight * nRatio;
      }
      nTmpWidth = o.ratio * nTmpWidth;
      nTmpHeight = o.ratio * nTmpHeight;
      // 图片调整居中
      var nTmpX = (nDomWidth - nTmpWidth) / 2;
      var nTmpY = (nDomHeight - nTmpHeight) / 2;
      o.mbg = $('motionbg');
      o.mbgsize = {};
      o.mbgsize.width = nTmpWidth;
      o.mbgsize.height = nTmpHeight;
      o.mbg.style.cssText = 'width:' + nTmpWidth + 'px; height:' + nTmpHeight + 'px;background-image:url(' + o.bgImg.src + ')';
      o.range = {}; // 边界
      if (o.orientation == 'landscape') {
        o.position.y = nTmpX;
        o.position.x = nTmpY;
      } else {
        o.position.x = nTmpX;
        o.position.y = nTmpY;
      }
      o.range.x = o.position.x * 2;
      o.range.y = o.position.y * 2;
      o.setPositionByOrien(o.position.x, o.position.y, true);
      o.mbg.style.visibility = 'visible';
      setTimeout(function () {
        $('motionMask').style.opacity = '0';
      }, 1000);
    }

    function fInit() {
      var o = this;
      // 检测方向
      o.getOrientation();
      // 设置背景图样式
      o.setBackground();
      // 阻止滚动
      document.documentElement.ontouchmove = function (e) {
        e.preventDefault();
      }
      // 检测
      if (window.DeviceMotionEvent == undefined) {
        // donothing
      } else {
        o.isDeviceMotion = true;
        // 绑定事件
        o.setMotionEvent();
        //o.setOriEvent();
      }
      window.onorientationchange = function () {
        o.getOrientation();
      };
      o.isInit = true;
    }

    function fGetElementById(s) {
      return document.getElementById(s);
    }

    oIndex.loginInit();
    oIndex.preload();

